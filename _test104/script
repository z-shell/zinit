tst_verbosity

unset ZCONVEY_ID ZCONVEY_FD 2>/dev/null

# The plugin has:
# - 8 aliases
# - 8 autoloads
# - 9 zle calls
# - 14 bindkeys
# - 4 setopts
# - 4 zstyles
# - 11 global parameters
zplugin load z-shell/zsh-editing-workbench

print -rl -- "${(o)ZPLG_ICE[@]}" >! answer/ice
print -rl -- "${(okv)ZPLG_SICE[@]}" >! answer/sice

store_state answer/state_bunload answer/errors_bunload

zplugin unload z-shell/zsh-editing-workbench

print $LASTREPORT > answer/report.txt

return 0

# vim:ft=zsh
