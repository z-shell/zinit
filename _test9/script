tst_verbosity

zinit ice depth"1" atclone'builtin echo >! test_file' atpull'%atclone'
zinit load z-shell/zsh-select

print -rl -- "${(o)ZPLG_ICE[@]}" >! answer/ice
print -rl -- "${(okv)ZPLG_SICE[@]}" >! answer/sice

perl -pi -e 's/test_plugins\/zsh-select/test_plugins\/zsh-select-more/g' answer/plugins/z-shell---zsh-select/.git/config

( zinit cd z-shell/zsh-select
  set -e
  print 'builtin echo >! Test_has_failed' >! ._zinit/atpull
  print -rl -- test_file* >! file.lst
  [[ -f test_file ]] && command rm -f test_file ) || return 1

zinit update z-shell/zsh-select

( zinit cd z-shell/zsh-select
  git log --oneline >! git_log )

