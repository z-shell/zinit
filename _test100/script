tst_verbosity

zinit ice depth"1"
zinit load z-shell/zsh-select
zinit load z-shell/ice-order

print -rl -- "${(o)ZPLG_ICE[@]}" >! answer/ice
print -rl -- "${(okv)ZPLG_SICE[@]}" >! answer/sice

zinit list > answer/loaded
zinit compiled > answer/compiled

local -a lines
lines=( "${(f)"$( zinit zstatus )"}" )
lines=( "${lines[@]/*\/test/}" )
print -rl -- "${lines[@]}" > answer/zstatus

zinit csearch > answer/csearch

